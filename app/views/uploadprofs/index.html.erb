<html>
<% if user_signed_in? %>
<h1>Listing uploadprofs</h1>
<%= will_paginate @uploadprofs %>

<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Resume</th>
    <th>Photo</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @uploadprofs.each do |uploadprof| %>
  <tr>
    <td><%= uploadprof.name %></td>
    <td><%= uploadprof.email %></td>
    <td><%= uploadprof.resume %></td>
    <td><%= uploadprof.photo %></td>
    <td><%= link_to 'Show', uploadprof %></td>
    <td><%= link_to 'Edit', edit_uploadprof_path(uploadprof) %></td>
    <td><%= link_to 'Destroy', uploadprof, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />
<% if current_user.email == "admin@gmail.com"%>
<%= link_to 'New Uploadprof', new_uploadprof_path %>
<%end%>

<% end %>
</html>